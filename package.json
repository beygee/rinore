{
  "name": "rinore",
  "version": "0.4.2",
  "description": "Rich Node.js REPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/rinore.git"
  },
  "keywords": [
    "repl"
  ],
  "author": "Sangmin Yoon <sangmin.yoon@croquis.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/croquiscom/rinore/issues"
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rimraf lib && tsc -p src/tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "test": "mocha --require ts-node/register -t 5000 --exit 'test/*.ts'",
    "test:cov": "nyc npm run test",
    "test:coveralls": "npm run test:cov && cat coverage/lcov.info | coveralls"
  },
  "bin": {
    "rinore": "./bin/rinore",
    "rinore-remote": "./bin/rinore-remote"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "bluebird": "^3.5.2",
    "diff": "^3.5.0",
    "lodash": "^4.17.11",
    "node-watch": "^0.5.8",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.7",
    "@types/diff": "^3.5.1",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.36",
    "@types/yargs": "^12.0.1",
    "chai": "^4.2.0",
    "coffeescript": "^2.3.2",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.5"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov"
    ]
  }
}
