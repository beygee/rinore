{
  "name": "rinore",
  "version": "0.3.0",
  "description": "Rich Node.js REPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/croquiscom/rinore.git"
  },
  "keywords": [
    "repl"
  ],
  "author": "Sangmin Yoon <sangmin.yoon@croquis.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/croquiscom/rinore/issues"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "build": "rimraf lib && tsc -p src/tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "test": "mocha --compilers ts:ts-node/register -t 5000",
    "test:cov": "nyc npm run test",
    "test:coveralls": "npm run test:cov && cat coverage/lcov.info | coveralls"
  },
  "bin": {
    "rinore": "./bin/rinore"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "bluebird": "^3.5.0",
    "diff": "^3.3.0",
    "lodash": "^4.17.4",
    "node-watch": "^0.5.5",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/chai": "^4.0.3",
    "@types/diff": "^3.2.0",
    "@types/lodash": "^4.14.72",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.20",
    "@types/yargs": "^8.0.2",
    "chai": "^4.1.1",
    "coffee-script": "^1.12.7",
    "coveralls": "^2.13.1",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov"
    ]
  }
}
